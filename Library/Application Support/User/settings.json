    "gopls": { "ui.semanticTokens": true },
    "microsoft-authentication.implementation": "msal",
    "notebook.lineNumbers": "on",
    "python.testing.pytestEnabled": true,
    "search.showLineNumbers": true,
    "scm.inputFontSize": 14,
    "terminal.external.osxExec": "WezTerm.app",
    "terminal.integrated.defaultProfile.osx": "zsh",
    "terminal.integrated.fontSize": 14,
    "vscode-neovim.neovimInitVimPaths.darwin": "/Users/st_lim/.config/nvim/init.lua",
    "vscode-neovim.revealCursorScrollLine": true,
    "window.commandCenter": true,
    "workbench.colorTheme": "Gruvbox Dark Hard",
    "workbench.preferredDarkColorTheme": "Gruvbox Dark Hard",
    "workbench.preferredHighContrastColorTheme": "Gruvbox Dark Hard",
    "workbench.preferredHighContrastLightColorTheme": "Gruvbox Dark Hard",
    "workbench.preferredLightColorTheme": "Gruvbox Light Hard",
}