{
    "application.shellEnvironmentResolutionTimeout": 20,
    "chat.editor.fontSize": 14,
    "cursor.cmdk.useThemedDiffBackground": true,
    "cursor.general.gitGraphIndexing": "enabled",
    "debug.console.fontSize": 14,
    "diffEditor.codeLens": true,
    "editor.accessibilitySupport": "off",
    "editor.detectIndentation": false,
    "editor.fontFamily": "'JetBrainsMonoNL Nerd Font',Menlo, Monaco, 'Courier New', monospace ",
    "editor.fontLigatures": true,
    "editor.fontSize": 14,
    "editor.guides.bracketPairs": true,
    "editor.lineNumbers": "relative",
    "editor.tabSize": 2,
    "editor.wordWrap": "on",
    "editor.wordWrapColumn": 120,
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "extensions.ignoreRecommendations": true,
    "files.trimTrailingWhitespace": true,
    "github.gitProtocol": "ssh",
    "github.codespaces.allowUnsafeConnections": true,
    "go.diagnostic.vulncheck": "Imports",
