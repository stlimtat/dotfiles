#!/bin/zsh
#set -x
# Android NDK Home
export ANDROID_NDK_HOME="/usr/local/share/android-ndk"

# aws completer
start_time=$(gdate +%s%3N)
complete -C '/opt/homebrew/bin/aws_completer' aws
end_time=$(gdate +%s%3N)
time_taken=$((end_time - start_time))
echo "Load aws completion init...${time_taken}ms...Done"

# Using colima
start_time=$(gdate +%s%3N)
source <($(colima completion zsh))
end_time=$(gdate +%s%3N)
time_taken=$((end_time - start_time))
echo "Load colima...${time_taken}ms...Done"

# Using docker
start_time=$(gdate +%s%3N)
source <($(docker completion zsh))
end_time=$(gdate +%s%3N)
time_taken=$((end_time - start_time))
echo "Load docker...${time_taken}ms...Done"

# fzf-git
start_time=$(gdate +%s%3N)
source ${HOME}/.config/fzf-git.sh/fzf-git.sh
end_time=$(gdate +%s%3N)
time_taken=$((end_time - start_time))
echo "Load fzf-git...${time_taken}ms...Done"

# Using kubectl 
start_time=$(gdate +%s%3N)
eval "$(kubectl completion zsh)"
end_time=$(gdate +%s%3N)
time_taken=$((end_time - start_time))
echo "Load kubectl...${time_taken}ms...Done"

# Using mise 
start_time=$(gdate +%s%3N)
eval "$(mise activate zsh)"
eval "$(mise completion zsh)"
end_time=$(gdate +%s%3N)
time_taken=$((end_time - start_time))
echo "Load mise...${time_taken}ms...Done"

# For jediterm - intellij terminal
if [ "$TERMINAL_EMULATOR" != "JetBrains-JediTerm" ]; then
  export ZSH_TMUX_AUTOSTART=true
else
  export PROMPT_COMMAND="resize &>/dev/null ; $PROMPT_COMMAND"
fi

# Using go
start_time=$(gdate +%s%3N)
export GOPATH=${HOME}/go
[[ "$(echo ${PATH} | grep ${GOPATH})" == "" ]] && \
  export PATH=${GOPATH}/bin:${PATH}
end_time=$(gdate +%s%3N)
time_taken=$((end_time - start_time))
echo "Load go...${time_taken}ms...Done"

# For jediterm - intellij terminal
# Using kubectl
export KUBECTL_EXTERNAL_DIFF="colordiff -N -u"
