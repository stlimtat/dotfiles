{
  "[go]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": "always"
    }
  },
  "containers.composeCommand": "nerdctl compose",
  "containers.containerClient": "com.microsoft.visualstudio.containers.docker",
  "containers.containerCommand": "nerdctl",
  "containers.orchestratorClient": "com.microsoft.visualstudio.orchestrators.dockercompose",
  "editor.accessibilitySupport": "off",
  "editor.fontFamily": "JetBrainsMonoNL Nerd Font, monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.fontWeight": "normal",
  "editor.formatOnSave": true,
  "editor.insertSpaces": true,
  "editor.lineHeight": 20,
  "editor.lineNumbers": "relative",
  "editor.minimap.enabled": true,
  "editor.tabSize": 2,
  "extensions.ignoreRecommendations": true,
  "github.gitProtocol": "ssh",
  "go.addTags": {
    "options": "json=omitempty,bson=omitempty,koanf=omitempty",
    "tags": "json,bson,koanf"
  },
  "go.addTags": {
    "options": "json=omitempty,bson=omitempty,koanf=omitempty",
    "tags": "json,bson,koanf"
  },
  "go.alternateTools": {
    "customFormatter": "golangci-lint"
  },
  "go.coverShowCounts": true,
  "go.diagnostic.vulncheck": "Imports",
  "go.formatFlags": [
    "fmt",
    "--stdin"
  ],
  "go.formatTool": "gofumpt",
  "go.lintFlags": [
    "--path-mode=abs",
    "--fast-only"
  ],
  "go.lintTool": "golangci-lint",
  "go.testEnvFile": "${workspaceFolder}/.env",
  "go.testEnvVars": {
    "HTTP_PROXY": "http://127.0.0.1:8080",
    "HTTPS_PROXY": "http://127.0.0.1:8080"
  },
  "go.testExplorer.packageDisplayMode": "nested",
  "gopls": {
    "ui.semanticTokens": true
  },
  "jupyter.askForKernelRestart": false,
  "notebook.defaultFormatter": "anysphere.cursorpyright",
  "notebook.diff.overviewRuler": true,
  "notebook.formatOnSave.enabled": true,
  "notebook.insertToolbarLocation": "notebookToolbar",
  "notebook.lineNumbers": "on",
  "notebook.output.wordWrap": true,
  "redhat.telemetry.enabled": true,
  "terminal.integrated.defaultProfile.osx": "zsh",
  "#terminal.integrated.profiles.osx#": "zsh",
  "testing.followRunningTest": true,
  "window.commandCenter": true,
  "workbench.colorTheme": "Gruvbox Material Dark",
  "cursor.composer.shouldChimeAfterChatFinishes": true,
  "cursor.enable_agent_window_setting": true,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  }
}